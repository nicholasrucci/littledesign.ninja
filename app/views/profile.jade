extends ./header.jade

block title
	title Little Design Ninja | Profile
block content
	div.spacer
	section.favorite-table
		div.wrapper
			h2 Favorites
			-var favs = ['fav','vid','goes','in','this','epic','long','list','cuz','why','not','right','?','?']

			-var favinfo = ['title', 'duration', 'category', 'x']
				table
					each favorite, index in favs
						tr
							each x, index in favinfo
								td= x
				div.new-vids
					-var recentvids = ['the', 'four', 'newest', 'videos']
					each vid in recentvids
						img(src=vid)
						p title
						p description
				div.clear
	div.spacer





	include ./foot.jade